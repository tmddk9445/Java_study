package chapter03;

	public class test {

		public class Main {
		    public static void main(String[] args) {
		    	
		        String[] arr = new String[5];
		        
		        for (int i = 0; i < arr.length; i += 2) {
						arr[i] = "hi";
				}
		        
		        for (int i = 0; i < arr.length; i++) {
		            System.out.println(arr[i]);
		        }
		    }
		}
		
	}
